{
    if ((mario.col > 6 && mario.col < 18 && mario.row > 160 - 70 && mario.row < 160 - 48) || (mario.col > 240 - 18 - mario.width && mario.col < 240 - 6 - mario.width && mario.row > 160 - 70 - 48 && mario.row < 160 - 48 - 48))
