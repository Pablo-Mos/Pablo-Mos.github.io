void updateWeapon()
{
    for (int i = 0; i < 3; i++)
    {
        if (collision(player.worldCol, player.worldRow, player.width, player.height, weapon[i].worldCol, weapon[i].worldRow, weapon[i].width, weapon[i].height))
        {
            if (weapon[i].active)
            {
                weapon[i].active = 0;
                numWeapons++;
                if (numWeapons == 1)
                    currWeapon = weapon[i].type;

                if (weapon[i].type == GUN)
                {
                    hasGun = 1;
                }
                else if (weapon[i].type == KNIFE)
                {
                    hasKnife = 1;
                }
                else if (weapon[i].type == SHIELD)
                {
                    hasShield = 1;
                }
            }
        }
    }
    for (int i = 0; i < 3; i++)
    {
        if (weapon[i].active == 1 && onScreen1(weapon[i]))
        {
            shadowOAM[i + 15].attr0 = (0xFF & (weapon[i].worldRow - vOff)) | (0 << 13) | (0 << 14);
            shadowOAM[i + 15].attr1 = (0x1FF & (weapon[i].worldCol - hOff)) | (0 << 14);
            if (weapon[i].type == GUN)
                shadowOAM[i + 15].attr2 = ((0) << 12) | ((2)*32 + (22));
            else if (weapon[i].type == KNIFE)
                shadowOAM[i + 15].attr2 = ((0) << 12) | ((2)*32 + (21));
            else if (weapon[i].type == SHIELD)
                shadowOAM[i + 15].attr2 = ((0) << 12) | ((2)*32 + (20));
        }
        else
        {
            shadowOAM[i + 15].attr0 = (2 << 8);
        }
    }
}


void updateEnemy()
{
    ovrEnemyColl = 0;
    for (int i = 0; i < 4; i++)
    {

        if (!successfulStab && enemy[i].active && stabbing && (collision(enemy[i].worldRow, enemy[i].worldCol, enemy[i].height, enemy[i].width, player.worldRow, player.worldCol + 5, player.height, player.width) || collision(enemy[i].worldRow, enemy[i].worldCol, enemy[i].height, enemy[i].width, player.worldRow, player.worldCol - 5, player.height, player.width)))
        {
            enemy[i].health--;
            successfulStab = 1;


            if (collision(enemy[i].worldRow, enemy[i].worldCol, enemy[i].height, enemy[i].width, player.worldRow, player.worldCol + 5, player.height, player.width))
                enemy[i].worldCol += 10;
            else
                enemy[i].worldCol -= 10;






        }


        if (enemy[i].active && enemy[i].health <= 0)
        {
            enemy[i].active = 0;
            pts++;
        }


        if (enemy[i].x <= 0)
        {
            enemy[i].direction = 1;

            enemy[i].shot = 1;
        }
        else if (enemy[i].x >= enemy[i].xLimit)
        {
            enemy[i].direction = -1;

            enemy[i].shot = 1;
        }
        else
            enemy[i].shot = 0;

        if (enemy[i].shot)
        {

            fireBulletEnemy(enemy[i]);


        }

        if (timer % 10 == 0)
        {
            if (enemy[i].direction == 1)
            {
                enemy[i].worldCol++;
                enemy[i].x++;
            }
            else
            {
                enemy[i].worldCol--;
                enemy[i].x--;
            }
        }

        if (enemy[i].aniCounter % 18 == 0)
        {
            enemy[i].curFrame = (enemy[i].curFrame + 1) % enemy[i].numFrames;
        }
        enemy[i].aniCounter++;
        if (enemy[i].active && (onScreen1(enemy[i]) && i < 3 ))
        {
            shadowOAM[i + 3 + 15].attr0 = (0xFF & (enemy[i].worldRow - vOff)) | (0 << 13) | (2 << 14);
            shadowOAM[i + 3 + 15].attr1 = (0x1FF & (enemy[i].worldCol - hOff)) | (2 << 14);
            if (enemy[i].shot)
            {
                if (enemy[i].type == NORMAL)
                {
                    if (enemy[i].direction == 1)
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((12)*32 + (8));
                    else
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((12)*32 + (12));
                }
                else
                {
                    if (enemy[i].direction == 1)
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((28)*32 + (8));
                    else
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((28)*32 + (12));
                }
            }

            else
            {
                if (enemy[i].type == NORMAL)
                {
                    if (enemy[i].direction == 1)
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((enemy[i].curFrame * 4)*32 + (8));
                    else
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((enemy[i].curFrame * 4)*32 + (12));
                }
                else if (enemy[i].type == BOSS)
                {
                    if (enemy[i].direction == 1)
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((enemy[i].curFrame * 4 + 16)*32 + (8));
                    else
                        shadowOAM[i + 3 + 15].attr2 = ((0) << 12) | ((enemy[i].curFrame * 4 + 16)*32 + (12));
                }

                if (collision(player.worldRow, player.worldCol, player.height, player.width, enemy[i].worldRow, enemy[i].worldCol, enemy[i].height, enemy[i].width))
                {
                    ovrEnemyColl = 1;
                    if (lives == 3)
                    {
                        lives--;
                        playSoundB(playerHurt_data, playerHurt_length, 0);
                    }
                    else if (lives == 2 && collTimer % 100 == 0)
                    {
                        lives--;
                        playSoundB(playerHurt_data, playerHurt_length, 0);
                    }
                    else if (lives == 1 && collTimer % 100 == 0)
                    {
                        lives--;
                        playSoundB(playerHurt_data, playerHurt_length, 0);
                    }
                    collTimer++;
                }
            }
        }
        else
            shadowOAM[i + 3 + 15].attr0 = (2 << 8);
    }
}


