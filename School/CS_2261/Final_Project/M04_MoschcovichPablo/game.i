}

void switchWeapons()
{
    if (currWeapon == SHIELD)
    {
        if (hasKnife)
        {
            currWeapon = KNIFE;
        }
        else if (hasGun)
        {
            currWeapon = GUN;
        }
    }
    else if (currWeapon == KNIFE)
    {
        if (hasGun)
        {
            currWeapon = GUN;
        }
        else if (hasShield)
        {
            currWeapon = SHIELD;
        }
    }
    else if (currWeapon == GUN)
    {
        if (hasShield)
        {
            currWeapon = SHIELD;
        }
        else if (hasKnife)
        {
            currWeapon = KNIFE;
        }
    }
}

