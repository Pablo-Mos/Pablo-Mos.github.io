void oBcheck()
{
    if (fallen == 0 && jumped == 0)
    {
        overBarrel = 0;
    }
    if (!overBarrel)
    {
        for (int i = 0; i < 4; i++)
        {
            if ((mario.col <= barrel[i].col + 4 && mario.col >= barrel[i].col - 4) && mario.row < barrel[i].row && barrel[i].row < mario.row + 50)
            {
                if (pts < 99 && barrel[i].active)
                {
