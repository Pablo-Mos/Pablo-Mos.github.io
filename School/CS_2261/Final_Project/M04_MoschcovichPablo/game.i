void updateBullet(ANISPRITE *b)
{


    if (b->active)
    {

        if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6))))
        {
            b->rowDirection = -1;
        }
        else if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 7))))
        {
            b->rowDirection = 1;
        }
        else
        {
            b->rowDirection = 0;
        }

        if (b->direction == 1)
        {
            if (b->worldCol + b->cdel <= 1024 - 1)
            {
                b->worldCol += b->cdel;
            }
            else
            {
                b->active = 0;
            }
        }
        else if (b->direction == -1)
        {
            if (b->worldCol + b->width - b->cdel >= 0)
            {
                b->worldCol -= b->cdel;
            }
            else
            {
                b->active = 0;
            }
        }
        else
        {
            if (b->worldRow + b->height - b->rdel >= 0)
            {
                b->worldRow -= b->rdel;
            }
            else
            {
                b->active = 0;
            }
        }

        if (b->direction != 0)
        {
            b->worldRow += b->rowDirection;
        }


        for (int i = 0; i < 4; i++)
        {
            if (enemy[i].active && collision(enemy[i].worldRow, enemy[i].worldCol, enemy[i].height, enemy[i].width,
                                             b->worldRow, b->worldCol, b->height, b->width))
            {
                b->active = 0;
                enemy[i].health--;
            }
        }


        if (collision(24, 992, 8, 32, b->worldRow, b->worldCol, b->height, b->width))
        {
            ((screenblock *)0x6000000)[31].tilemap[((3) * (32) + (29))] = 0;
            ((screenblock *)0x6000000)[31].tilemap[((3) * (32) + (30))] = 0;
            ((screenblock *)0x6000000)[31].tilemap[((3) * (32) + (31))] = 0;
            b->active = 0;
            keyFall = 1;
        }
    }
}

void updateBulletEnemy(ANISPRITE *b)
{

    if (b->active)
    {
        if (b->direction == 1)
        {
            if (b->worldCol + b->cdel <= 1024 + hOff - 1)
            {
                b->worldCol += b->cdel;
            }
            else
            {
                b->active = 0;
            }
        }
        else if (b->direction == -1)
        {
            if (b->worldCol + b->width - b->cdel >= hOff)
            {
                b->worldCol -= b->cdel;
            }
            else
            {
                b->active = 0;
            }
        }



        if (collision(player.worldRow, player.worldCol, player.height, player.width,
                      b->worldRow, b->worldCol, b->height, b->width))
        {
            b->active = 0;
            if (!shielding)
            {
                lives--;
                playSoundB(playerHurt_data, playerHurt_length, 0);
            }
        }
    }
}


void updatePlayer()
{

    if ((!(~(oldButtons) & ((1 << 2))) && (~buttons & ((1 << 2)))) && numWeapons > 1)
    {
        switchWeapons();
    }
    if (!ovrEnemyColl)
    {
        collTimer = 100;
    }

    if (hasKnife && stabbingFrames < 10)
    {
        stabbingFrames++;
        stabbing = 1;
    }
    else
    {
        stabbing = 0;
        successfulStab = 0;
    }

    if (timer % 45 == 0)
        player.shot = 0;







    if (currWeapon == GUN && (!(~(oldButtons) & ((1 << 0))) && (~buttons & ((1 << 0)))) && player.bulletTimer >= 10 && numBullets > 0)
    {
        fireBullet();
        player.bulletTimer = 0;
    }
    player.bulletTimer++;


    if (currWeapon == KNIFE && (!(~(oldButtons) & ((1 << 0))) && (~buttons & ((1 << 0)))))
    {
        stabbing = 1;
        stabbingFrames = 0;
    }


    if (currWeapon == SHIELD && ((!(~(oldButtons) & ((1 << 0))) && (~buttons & ((1 << 0)))) || (~((*(volatile unsigned short *)0x04000130)) & ((1 << 0)))))
    {
        shielding = 1;
    }
    else
        shielding = 0;


    if (player.aniState != SPRITEIDLE)
    {
        player.prevAniState = player.aniState;
        player.aniState = SPRITEIDLE;
    }
    if (player.aniCounter % 6 == 0)
    {
        if (player.curFrame < player.numFrames - 1)
        {
            player.curFrame++;
        }
        else
        {
            player.curFrame = 0;
        }
    }


    if (((!(~(oldButtons) & ((1 << 1))) && (~buttons & ((1 << 1)))) || (~((*(volatile unsigned short *)0x04000130)) & ((1 << 1)))) && jumped == 0 && fallen == 0)
    {
        jumped = 1;
        jumpCount = 0;
        player.rdel = -1;
    }
    if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5))) && (player.worldCol > 0) && collisionCheck(collisionMap, 1024, player.worldCol, player.worldRow, player.width, player.height, -player.cdel, 0))
    {
        player.aniState = SPRITELEFT;
        player.worldCol -= player.cdel;

        if ((screenBlock > 28 || hOff > 0) && (player.worldCol - playerhOff <= 240 / 2))
        {
            hOff--;
            playerhOff -= player.cdel;
        }

        else if ((screenBlock > 28 || hOff > 0) && (player.worldCol - playerhOff - 1024 / 2 <= 240 / 2))
        {
            hOff--;
            playerhOff -= player.cdel;
        }
    }
    else if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4))) && (player.worldCol + player.width - 1 < 1024 - 6) && collisionCheck(collisionMap, 1024, player.worldCol, player.worldRow, player.width, player.height, player.cdel, 0))
    {
        player.aniState = SPRITERIGHT;
        player.worldCol += player.cdel;
        if ((screenBlock < 31 || hOff < 32) && hOff + 240 <= 1024 && player.worldCol - playerhOff >= 240 / 2)
        {
            if (!(screenBlock == 30 && playerhOff == 272))
            {
                hOff++;
                playerhOff += player.cdel;
            }
        }
    }


    if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6))))
        shootUp = 1;
    else
        shootUp = 0;


    drawPlayer();


    if (player.aniState != SPRITEIDLE)
        player.aniCounter++;


    if (!jumped && collisionMap[((player.worldRow + player.height) * (1024) + (player.worldCol))] != 0 && collisionMap[((player.worldRow + player.height) * (1024) + (player.worldCol + (player.width / 2)))] != 0 && collisionMap[((player.worldRow + player.height) * (1024) + (player.worldCol + player.width - 1))] != 0)
        player.worldRow++;
}


void fireBullet()
{
    player.shot = 1;
    numBullets--;

    for (int i = 0; i < 5; i++)
    {
        if (!bullets[i].active)
        {

            if (shootUp)
            {
                bullets[i].worldRow = player.worldRow - bullets[i].width;
                bullets[i].worldCol = player.worldCol;
                bullets[i].direction = 0;

                bullets[i].height = 13;
                bullets[i].width = 5;
            }
            else if (player.aniState == SPRITELEFT || player.aniState == SPRITEIDLE && player.prevAniState == SPRITELEFT)
            {
                bullets[i].worldRow = player.worldRow + player.height / 2 - 3;
                bullets[i].worldCol = player.worldCol - bullets[i].width / 2;
                bullets[i].direction = -1;

                bullets[i].height = 5;
                bullets[i].width = 13;
            }
            else
            {
                bullets[i].worldRow = player.worldRow + player.height / 2 - 3;
                bullets[i].worldCol = player.worldCol + player.width - bullets[i].width / 2;
                bullets[i].direction = 1;

                bullets[i].height = 5;
                bullets[i].width = 13;
            }


            bullets[i].active = 1;


            break;
        }
    }
}


void drawBullets()
{
    for (int i = 0; i < 5; i++)
    {
        if (bullets[i].active )
        {
            if (bullets[i].direction != 0)
            {
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr0 = (0xFF & (bullets[i].worldRow - vOff)) | (0 << 13) | (0 << 14);
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr1 = (0x1FF & (bullets[i].worldCol - hOff)) | (0 << 14);
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr2 = ((0) << 12) | ((0)*32 + (20));
            }
            else
            {
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr0 = (0xFF & (bullets[i].worldRow - vOff)) | (0 << 13) | (0 << 14);
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr1 = (0x1FF & (bullets[i].worldCol - hOff)) | (0 << 14);
                shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr2 = ((0) << 12) | ((1)*32 + (20));
            }
        }
        else
        {
            shadowOAM[i + 3 + 4 + 3 + 5 + 15].attr0 = (2 << 8);
        }
    }
}

void drawBulletsEnemy()
{
    for (int i = 0; i < 30; i++)
    {
        if (!onScreen1(enemyBullets[i]))
        {
            enemyBullets[i].active = 0;
        }

        if (enemyBullets[i].active && onScreen1(enemyBullets[i]))
        {
            shadowOAM[i + 3 + 4 + 3 + 5 + 5 + 15].attr0 = (0xFF & (enemyBullets[i].worldRow - vOff)) | (0 << 13) | (0 << 14);
            shadowOAM[i + 3 + 4 + 3 + 5 + 5 + 15].attr1 = (0x1FF & (enemyBullets[i].worldCol - hOff)) | (0 << 14);
            shadowOAM[i + 3 + 4 + 3 + 5 + 5 + 15].attr2 = ((0) << 12) | ((0)*32 + (21));
        }
        else
        {
            shadowOAM[i + 3 + 4 + 3 + 5 + 5 + 15].attr0 = (2 << 8);
        }
    }
}

void fireBulletEnemy(ANISPRITE e)
{

    for (int i = 0; i < 30; i++)
    {
        if (!enemyBullets[i].active && timer % 20 == 0 && e.active)
        {
            enemyBullets[i].worldRow = e.worldRow + e.height / 2 - 3;
            enemyBullets[i].worldCol = e.worldCol ;
            enemyBullets[i].height = 5;
            enemyBullets[i].width = 13;
            if (e.direction == -1)
            {
                enemyBullets[i].direction = -1;
            }
            else
            {
                enemyBullets[i].direction = 1;
            }


            enemyBullets[i].active = 1;


            break;
        }
    }
}

void drawPlayer()
{
    shadowOAM[0].attr0 = (0xFF & (player.worldRow - vOff)) | (0 << 13) | (2 << 14);
    shadowOAM[0].attr1 = (0x1FF & (player.worldCol - playerhOff)) | (2 << 14);
    if (jumped)
    {
        if (player.prevAniState == SPRITERIGHT || player.aniState == SPRITERIGHT)
            shadowOAM[0].attr2 = ((0) << 12) | ((12)*32 + (0));
        else if (player.prevAniState == SPRITELEFT || player.aniState == SPRITELEFT)
            shadowOAM[0].attr2 = ((0) << 12) | ((12)*32 + (4));
        if (timer % 2 == 0)
        {
            player.worldRow -= 2;
            jumpCount++;

            if (jumpCount == maxJump || (collisionMap[((player.worldRow - 1) * (1024) + (player.worldCol))] == 0 || collisionMap[((player.worldRow - 1) * (1024) + (player.worldCol + (player.width / 2)))] == 0 || collisionMap[((player.worldRow - 1) * (1024) + (player.worldCol + player.width - 1))] == 0))
            {
                jumped = 0;
                fallen = 1;
            }
        }
    }
    else if (collisionCheck(collisionMap, 1024, player.worldCol, player.worldRow + 2, player.width, player.height, 0, player.rdel))
        player.worldRow++;
    else if (fallen)
    {
        if (player.prevAniState == SPRITERIGHT || player.aniState == SPRITERIGHT)
            shadowOAM[0].attr2 = ((0) << 12) | ((12)*32 + (0));
        else if (player.prevAniState == SPRITELEFT || player.aniState == SPRITELEFT)
            shadowOAM[0].attr2 = ((0) << 12) | ((12)*32 + (4));

        if (timer % 1 == 0)
        {
            jumpCount--;
            if (jumpCount == 0)
            {
                fallen = 0;
            }
        }
    }

    else if (stabbing)
    {
        if (player.prevAniState == SPRITERIGHT || player.aniState == SPRITERIGHT)
            shadowOAM[0].attr2 = ((0) << 12) | ((16)*32 + (0));
        else if (player.prevAniState == SPRITELEFT || player.aniState == SPRITELEFT)
            shadowOAM[0].attr2 = ((0) << 12) | ((16)*32 + (4));
    }

    else if (shielding)
    {
        if (player.prevAniState == SPRITERIGHT || player.aniState == SPRITERIGHT)
            shadowOAM[0].attr2 = ((0) << 12) | ((24)*32 + (0));
        else if (player.prevAniState == SPRITELEFT || player.aniState == SPRITELEFT)
            shadowOAM[0].attr2 = ((0) << 12) | ((24)*32 + (4));
    }
    else if (player.shot)
    {
        if (player.prevAniState == SPRITERIGHT || player.aniState == SPRITERIGHT)
            shadowOAM[0].attr2 = ((0) << 12) | ((20)*32 + (0));
        else if (player.prevAniState == SPRITELEFT || player.aniState == SPRITELEFT)
            shadowOAM[0].attr2 = ((0) << 12) | ((20)*32 + (4));
    }
    else if (player.prevAniState == SPRITERIGHT && player.aniState != SPRITERIGHT)
        shadowOAM[0].attr2 = ((0) << 12) | ((0)*32 + (0));
    else if (player.prevAniState == SPRITELEFT && player.aniState != SPRITELEFT)
        shadowOAM[0].attr2 = ((0) << 12) | ((0)*32 + (4));
    else if (player.aniState == SPRITERIGHT)
        shadowOAM[0].attr2 = ((0) << 12) | ((player.curFrame * 4)*32 + (0));
    else if (player.aniState == SPRITELEFT)
        shadowOAM[0].attr2 = ((0) << 12) | ((player.curFrame * 4)*32 + (4));
}

void updateCamera()
{
    if (player.worldRow > 256 / 2 + 16)
        vOff = 96;
